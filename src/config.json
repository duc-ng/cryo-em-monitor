{
  "app": {
    "maxDays": 7,
    "pollServerMs": 10000,
    "pollClientMs": 20000,
    "notification": {
      "ms": 300000,
      "message": "No data for 5 minutes."
    },
    "heapAllocation": "auto",
    "autodelete": {
      "isOn": true,
      "maxLogDays": 2,
      "maxDataSize": 50000
    }
  },

  "test": {
    "loopMs": 10,
    "folder": "Titan1"
  },

  "microscopes": [
    { "label": "Titan 1", "folder": "Titan1" },
    { "label": "Titan 2", "folder": "Titan2" },
    { "label": "Titan 3", "folder": "Titan3" },
    { "label": "Titan 4", "folder": "Titan4" },
    { "label": "Titan 5", "folder": "Titan5" }
  ],

  "key": "_mmsImageKey_Value",

  "times.star": [
    { "label": "Images acquired", "value": "_mmsdateAuqired_Value" },
    { "label": "Images imported", "value": "_mmsdateImported_Value" },
    { "label": "Images processed", "value": "_mmsdateProcessed_Value" },
    { "label": "Images exported", "value": "_mmsdateExported_Value" },
    { "label": "Images errors", "value": "_mmsdateError_Value" }
  ],

  "data.star": [
    {
      "label": "Mean",
      "description": "Pixel Values [counts/px]",
      "value": "_mmsmean_Value",
      "maxOptimum": 0.6,
      "minOptimum": 0.2
    },
    {
      "label": "Drift",
      "description": "Total Sample Drift [Å]",
      "value": "_mmsdrift_Value",
      "maxOptimum": 0.6,
      "minOptimum": 0.2
    },
    {
      "label": "Iciness",
      "description": "Iciness [a.u.]",
      "value": "_mmsiciness_Value",
      "maxOptimum": 0.9,
      "minOptimum": 0.1
    },
    {
      "label": "Defocus",
      "description": "Defocus Fit [μm]",
      "value": "_mmsdefocus_Value",
      "maxOptimum": 0.0,
      "minOptimum": 0.0
    },
    {
      "label": "Resolution",
      "description": "Resolution of CTF Fit [Å]",
      "value": "_mmsresolution_Value",
      "maxOptimum": 0.5,
      "minOptimum": 0.4
    },
    {
      "label": "CCvalue",
      "description": "CC Value of CTF Fit [a.u.]",
      "value": "_mmsccOfCtfFit_Value",
      "maxOptimum": 0.7,
      "minOptimum": 0.3
    },
    {
      "label": "Astigmatism",
      "description": "Astigmatism [μm]",
      "value": "_mmsastigmatism_Value",
      "maxOptimum": 0.9,
      "minOptimum": 0.0
    },
    {
      "label": "Particles",
      "description": "Number of Particles",
      "value": "_mmnumberOfParticles_Value",
      "maxOptimum": 10000,
      "minOptimum": 0.0
    }
  ],

  "images.star": [
    {
      "label": "mmsrawAvg",
      "value": "_mmsrawAvg_Name",
      "info": "_mmsrawAvg_Info"
    },
    {
      "label": "mmspsRawAvg",
      "value": "_mmspsRawAvg_Name",
      "info": "_mmspsRawAvg_Info"
    },
    {
      "label": "mmsmotionCorrAvg",
      "value": "_mmsmotionCorrAvg_Name",
      "info": "_mmsmotionCorrAvg_Info"
    },
    {
      "label": "mmspsMotionCorrAvg",
      "value": "_mmspsMotionCorrAvg_Name",
      "info": "_mmspsMotionCorrAvg_Info"
    },
    {
      "label": "mmsdriftplot",
      "value": "_mmsdriftplot_Name",
      "info": "_mmsdriftplot_Info"
    },
    {
      "label": "mmsctfdiag",
      "value": "_mmsctfdiag_Name",
      "info": "_mmsctfdiag_Info"
    },
    {
      "label": "mmspick",
      "value": "_mmspick_Name",
      "info": "_mmspick_Info"
    }
  ]
}
